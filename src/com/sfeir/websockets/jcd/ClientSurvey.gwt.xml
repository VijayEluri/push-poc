<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.4/distro-source/core/src/gwt-module.dtd">
<module rename-to="jcd">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.sfeir.websockets.ws.Websockets" />
	<inherits name="com.sfeir.websockets.users.Users" />

	<entry-point class="com.sfeir.websockets.jcd.client.ClientSurvey" />
	
	<source path='client'/>
	<source path='shared'/>
	<source path='view' />

	<set-property name="user.agent" value="safari"/>
	
</module>